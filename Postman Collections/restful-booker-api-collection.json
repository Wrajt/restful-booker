{
	"info": {
		"_postman_id": "bd7a47ec-1d09-47b4-98c5-f399b78f99bb",
		"name": "Restful-booker",
		"schema": "https://schema.getpostman.com/json/collection/v2.0.0/collection.json"
	},
	"item": [
		{
			"name": "Logowanie -> get token",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "1378385a-a2f9-4840-8b4d-6091408aac35",
						"exec": [
							"pm.test(\"status code is 200\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"})"
						],
						"type": "text/javascript"
					}
				}
			],
			"id": "2d4cb741-0ddb-48a6-9f6f-dfcfa024dee3",
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"username\": \"admin\",\r\n    \"password\": \"password123\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": "https://restful-booker.herokuapp.com/auth"
			},
			"response": []
		}
	],
	"auth": {
		"type": "bearer",
		"bearer": {
			"token": "{{BEARERTOKEN}}"
		}
	},
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"id": "407f4012-d41f-45db-a903-6f6c3247d9b6",
				"type": "text/javascript",
				"exec": [
					"pm.sendRequest({",
					"    url: \"https://restful-booker.herokuapp.com/booking\",",
					"    method: 'POST',",
					"    header: {",
					"        'Accept': 'application/json',",
					"        'Content-Type': 'application/x-www-form-urlencoded'",
					"    },",
					"    body: {",
					"    \"username\" : \"admin\",",
					"    \"password\" : \"password123\"",
					"}",
					"},",
					"    (err, res) => {",
					"        // Set BEARERTOKEN",
					"        pm.globals.set(\"BEARERTOKEN\", res.json().token)",
					"        // console.log(res.json());",
					"});"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"id": "dbff791e-31bf-47a1-b046-0372859e68df",
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		}
	]
}