	"item": [
		{
			"name": "Logowanie -> get token",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "1378385a-a2f9-4840-8b4d-6091408aac35",
						"exec": [
							"pm.test(\"status code is 200\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"})\r",
							"\r",
							"pm.test(\"get id\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"})\r",
							"\r",
							"\r",
							"pm.collectionVariables.set(\"variable_key\", \"variable_value\");"
						],
						"type": "text/javascript"
					}
				}
			],
			"id": "2d4cb741-0ddb-48a6-9f6f-dfcfa024dee3",
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"username\": \"admin\",\r\n    \"password\": \"password123\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": "https://restful-booker.herokuapp.com/auth"
			},
			"response": []
		},
		{
			"name": "Get Booking IDS",
			"id": "f9367e60-88d4-4ed7-b304-5ad82c6f1109",
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"method": "GET",
				"header": []
			},
			"response": []
		}
	],
	"auth": {
		"type": "bearer",
		"bearer": {
			"token": "{{BEARERTOKEN}}"
		}
	},
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"id": "2f8706bf-f655-4880-840f-95b8e0be67da",
				"type": "text/javascript",
				"exec": [
					"pm.sendRequest({",
					"    url: \"https://restful-booker.herokuapp.com/auth\",",
					"    method: 'POST',",
					"    header: {",
					"        'Accept': 'application/json',",
					"        'Content-Type': 'application/x-www-form-urlencoded'",
					"    },",
					"    body: {",
					"    \"username\" : \"admin\",",
					"    \"password\" : \"password123\"",
					"}",
					"}",
					")",
					"",
					"pm.collectionVariables.set(\"BEARERTOKEN\", pm.response.json().token)",
					"",
					"",
					"const tokenUrl = 'https://restful-booker.herokuapp.com/auth';",
					"",
					"const clientId = 'admin';",
					"const clientSecret = 'password123';",
					"",
					"const getTokenRequest = {",
					"  method: 'POST',",
					"  url: tokenUrl,",
					"  body: {",
					"      mode: 'formdata',",
					"      formdata: [",
					"          { key: 'grant_type', value: 'client_credentials' },",
					"          { key: 'client_id', value: clientId },",
					"          { key: 'client_secret', value: clientSecret }",
					"      ]",
					"  }",
					"};",
					"",
					"pm.sendRequest(getTokenRequest, (err, response) => {",
					"  const jsonResponse = response.json();",
					"  const newAccessToken = jsonResponse.token;",
					"",
					"  pm.variables.set('BEARERTOKEN', newAccessToken);",
					"});"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"id": "d6fb2429-f7e1-4825-bcf9-17a57c404b51",
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		}
	],
	"variable": [
		{
			"id": "d687602d-6de9-4410-a6b8-c65190f574b3",
			"key": "variable_key",
			"value": ""
		}
	]
}