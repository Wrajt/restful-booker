		}
	},
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"id": "c6867a77-5123-404d-8f7e-74f426a37c4b",
				"type": "text/javascript",
				"exec": [
					"pm.sendRequest({",
					"    url: \"https://restful-booker.herokuapp.com/auth\",",
					"    method: 'POST',",
					"    header: {",
					"        'Accept': 'application/json',",
					"        'Content-Type': 'application/x-www-form-urlencoded'",
					"    },",
					"    body: {",
					"    \"username\" : \"admin\",",
					"    \"password\" : \"password123\"",
					"}",
					"})",
					"",
					"pm.collectionVariables.set(\"BEARERTOKEN\", response.json().token)"
				]
			}
		},
